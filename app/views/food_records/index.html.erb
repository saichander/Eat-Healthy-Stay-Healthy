<table>
  <tr>
    <th>Item</th>
    <th>Quantity</th>
    <th>Intake_calories</th>
    <th>Edit</th>
  </tr>
  <% @food_records.each do |food_record| %>
    <tr>
      <td><%= food_record.item%></td>
      <td><%= food_record.quantity %></td>
      <td><%= food_record.intake_calories%></td>
      <td><%= link_to 'Edit',edit_food_record_path(food_record.id) %></td>
    </tr>
  <% end %>
