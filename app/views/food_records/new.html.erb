<%= form_for @food_record, url: create_food_record_path, class: 'form-horizontal' do |f| %>
  <%= f.label 'Item' %>
  <%= f.text_field :item, class:'form-control'%>
  <%= f.label 'Quantity' %>
  <%= f.number_field :quantity, class:'form-control' %>
  <%= f.label 'Intake date' %>
  <%= f.date_field :intake_date,class:'form-control' %>
  <%= f.submit 'Add Food Record',class:'btn btn-default' %>
<% end %>
